		<element id="Atom(MessageItem)" layout="BorderLayout()" contentalign="TopLeft" padding="rect(6rp, 16rp, 6rp, 16rp)">
            --><!--<ModernProgressRing layoutpos="client" minsize="size(50rp, 50rp)" id="Atom(SXWizardLoadingProgress)"   sheet="RefDark" />--><!--
			  
            <element layoutpos="top" class="WizardPageTitle" id="Atom(SXTitle)" content="Click the buttons to get started" />
